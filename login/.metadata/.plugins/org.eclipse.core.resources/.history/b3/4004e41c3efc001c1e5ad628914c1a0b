function loginChk() {
    var form = document.f1;
    if (!form.user_id.value) {
        alert("아이디를 입력해 주십시오.");
        form.user_id.focus();
        return;
    }
 
    if (!form.user_pw.value) {
        alert("비밀번호를 입력해 주십시오.");
        form.user_pw.focus();
        return;
    }
    form.submit();
}